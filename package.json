{
  "name": "@socialgorithm/uttt",
  "version": "4.3.2",
  "description": "Ultimate Tic Tac Toe Algorithm - Game Server Edit",
  "main": "dist/standalone.js",
  "scripts": {
    "start": "node dist/standalone.js",
    "test": "nyc mocha --recursive --compilers ts:ts-node/register --bail",
    "docs": "gulp typedoc && gulp noJekyll"
  },
  "bin": {
    "uttt": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aurbano/ultimate-ttt-server.git"
  },
  "keywords": [
    "ultimate",
    "tic",
    "tac",
    "toe",
    "server",
    "algorithm",
    "battle"
  ],
  "author": "Alejandro U. Alvarez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aurbano/ultimate-ttt-server/issues"
  },
  "homepage": "https://github.com/aurbano/ultimate-ttt-server#readme",
  "dependencies": {
    "@socialgorithm/ultimate-ttt": "^4.18.0",
    "@types/ip": "0.0.29",
    "@types/node": "^7.0.22",
    "@types/socket.io": "^1.4.29",
    "blessed": "^0.1.81",
    "command-line-args": "^4.0.5",
    "command-line-usage": "^4.0.0",
    "ip": "^1.1.5",
    "socket.io": "^2.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "ava": "^0.19.1",
    "babel": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "gulp": "^3.9.1",
    "gulp-file": "^0.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-typedoc": "^2.0.2",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2",
    "ts-node": "^3.0.4",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.3"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true
  }
}
