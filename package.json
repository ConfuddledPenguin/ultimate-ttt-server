{
  "name": "@socialgorithm/uttt",
  "version": "5.3.0",
  "description": "Ultimate Tic Tac Toe Algorithm - Game Server Edit",
  "main": "dist/standalone.js",
  "scripts": {
    "start": "node dist/standalone.js",
    "build": "tsc -w",
    "test": "nyc mocha --recursive --compilers ts:ts-node/register --bail",
    "docs": "gulp typedoc && gulp noJekyll"
  },
  "bin": {
    "uttt": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aurbano/ultimate-ttt-server.git"
  },
  "keywords": [
    "ultimate",
    "tic",
    "tac",
    "toe",
    "server",
    "algorithm",
    "battle"
  ],
  "author": "Alejandro U. Alvarez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aurbano/ultimate-ttt-server/issues"
  },
  "homepage": "https://github.com/aurbano/ultimate-ttt-server#readme",
  "dependencies": {
    "@socialgorithm/ultimate-ttt": "^4.19.0",
    "@types/ip": "0.0.31",
    "@types/node": "^10.5.1",
    "@types/socket.io": "^1.4.36",
    "blessed": "^0.1.81",
    "command-line-args": "^4.0.6",
    "command-line-usage": "^4.0.0",
    "ip": "^1.1.5",
    "random-word": "^2.0.0",
    "socket.io": "^2.0.1"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.10",
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "gulp": "^3.9.1",
    "gulp-file": "^0.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-typedoc": "^2.0.2",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2",
    "socket.io-client": "^2.1.1",
    "ts-node": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.3.4"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true
  }
}
